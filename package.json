{
	"name": "clean-arch-luby-sls",
	"version": "1.0.0",
	"description": "Serverless aws-nodejs-typescript template",
	"main": "serverless.ts",
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watchAll --verbose",
		"test:integration": "jest \\.test\\.ts",
		"typeorm": "ts-node node_modules/typeorm/cli.js",
		"typeorm:seed:create": "npm run typeorm -- migration:create -c seeds -n",
		"typeorm:seed:run": "npm run typeorm -- migration:run -c seeds",
		"typeorm:test:migration:create": "npm run typeorm -- migration:create -c tests -n",
		"typeorm:test:migration:run": "npm run typeorm -- migration:run -c tests"
	},
	"engines": {
		"node": ">=14.17.0"
	},
	"dependencies": {
		"@middy/core": "^1.5.2",
		"@middy/http-json-body-parser": "^1.5.2",
		"bcrypt": "^5.0.1",
		"class-validator": "^0.13.1",
		"dotenv": "^10.0.0",
		"inversify": "^5.1.1",
		"mysql2": "^2.3.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.37"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@serverless/typescript": "^2.23.0",
		"@types/aws-lambda": "^8.10.71",
		"@types/bcrypt": "^5.0.0",
		"@types/jest": "^27.0.1",
		"@types/node": "^14.14.25",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import-helpers": "^1.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"jest": "^27.2.0",
		"prettier": "^2.4.1",
		"serverless": "^2.23.0",
		"serverless-offline": "^8.2.0",
		"serverless-webpack": "^5.3.5",
		"ts-jest": "^27.0.5",
		"ts-loader": "^8.0.15",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"tsconfig-paths-webpack-plugin": "^3.3.0",
		"typescript": "^4.1.3",
		"webpack": "^5.20.2",
		"webpack-node-externals": "^2.5.2"
	},
	"author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
	"license": "MIT"
}
